{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "ext-zip": "*",
        "apility/laravel-fcm": "^1.4",
        "awobaz/compoships": "^2.1",
        "barryvdh/laravel-translation-manager": "^0.6.2",
        "doctrine/dbal": "^3.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "goldspecdigital/laravel-eloquent-uuid": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "intervention/image": "^2.7",
        "intervention/imagecache": "^2.5",
        "laravel/framework": "^8.54",
        "laravel/passport": "^10.4",
        "laravel/telescope": "^4.9",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "opcodesio/log-viewer": "^1.2",
        "pda/pheanstalk": "^4.0",
        "predis/predis": "^1.1",
        "qiutuleng/laravel-passport-phone-verification-code-grant": "^2.0",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "signifly/laravel-builder-macros": "^1.4",
        "spatie/laravel-activitylog": "^4.5",
        "spatie/laravel-permission": "^5.1",
        "spatie/laravel-backup": "^7.8.0",
        "staudenmeir/eloquent-json-relations": "^1.1",
        "symfony/psr-http-message-bridge": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "matt-allan/laravel-code-style": "^0.7.0",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeds/"
        },
        "files": [
            "app/Helpers/generalHelper.php",
            "app/Helpers/projectHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Helpers/generalHelper.php",
            "app/Helpers/projectHelper.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
